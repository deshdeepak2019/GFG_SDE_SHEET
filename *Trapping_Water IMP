Bruteforce   -- O(N) and (2*N)

 long long trappingWater(int arr[], int n){
       long long int sum=0;
        vector<int>v1(n),v2(n);
        v1[0]=arr[0];
        v2[n-1]=arr[n-1];
        for(int i=1;i<n;i++){
            v1[i]=max(v1[i-1],arr[i]);
        }
        for(int i=n-2;i>=0;i--){
            v2[i]=max(v2[i+1],arr[i]);
        }
       for(int i=0;i<n;i++){
           sum+=(min(v1[i],v2[i])-arr[i]);
       }
        return sum;
    }
